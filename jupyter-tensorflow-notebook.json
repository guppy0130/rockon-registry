{
    "Jupyter Tensorflow Notebook": {
        "containers": {
            "tensorflow-notebook": {
                "image": "jupyter/tensorflow-notebook",
                "launch_order": 1,
                "ports": {
                    "8888": {
                        "description": "Port to access notebook over",
                        "host_default": 8888,
                        "protocol": "tcp",
                        "label": "Notebook Port",
                        "ui": true
                    }
                },
                "volumes": {
                    "/home/jovyan/work": {
                        "description": "Choose a share for your notebook data. Set permissions to 777 in access control.",
                        "label": "Notebook Storage",
                        "min_size": "10485760"
                    }
                }
            }
        },
        "description": "Jupyter notebook with Python deep learning libraries. Please view docker logs to obtain token for initial sign in, or <a href=\"https://github.com/jupyter/notebook/issues/2254\">view this Github thread for alternatives.</a>",
        "more_info": "Comes with tensorflow and keras.",
        "version": "0.0.1",
        "website": "http://jupyter.org"
    }
}
